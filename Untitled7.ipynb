   "source": [
    "mas = [0 for i in range(100)]\n",
    "\n",
    "def press(i):\n",
    "    global mas\n",
    "    if mas[i] == 0:\n",
    "        mas[i] = 1\n",
    "    elif mas[i] == 1:\n",
    "        mas[i] = 0\n",
    "        \n",
    "def frog(i):\n",
    "    k = i\n",
    "    while i <= 100:\n",
    "        press(i-1)\n",
    "        i += k\n"
   ]
